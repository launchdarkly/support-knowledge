{
  "name": "hello-react-native-expo",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "android-release": "expo run:android --variant release",
    "ios": "expo start --ios",
    "clean-reset": "yarn cache clean && rm -rf node_modules && rm -rf .expo && rm -rf yarn.lock",
    "watchman-reset": "watchman watch-del-all && watchman shutdown-server"
  },
  "dependencies": {
    "@launchdarkly/react-native-client-sdk": "10.9.3",
    "@react-native-community/cli-server-api": "^15.1.3",
    "expo": "~52.0.0",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.76.6",
    "react-native-dotenv": "^3.4.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "@types/react-native-dotenv": "^0.2.2",
    "typescript": "^5.2.2"
  },
  "private": true,
  "packageManager": "yarn@3.4.1"
}
